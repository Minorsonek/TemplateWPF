<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TemplateWPF">

    <!-- Coolors website - https://coolors.co/fefdfe-34363c-d7d5d7-57a598-64666c -->

    <Color x:Key="WhitePure">#ffffff</Color>
    <SolidColorBrush x:Key="WhitePureBrush" Color="{StaticResource WhitePure}" />

    <Color x:Key="WhiteLight">#FEFDFE</Color>
    <SolidColorBrush x:Key="WhiteLightBrush" Color="{StaticResource WhiteLight}" />

    <Color x:Key="WhiteDark">#D7D5D7</Color>
    <SolidColorBrush x:Key="WhiteDarkBrush" Color="{StaticResource WhiteDark}" />

    <Color x:Key="GreenLight">#57A598</Color>
    <SolidColorBrush x:Key="GreenLightBrush" Color="{StaticResource GreenLight}" />

    <Color x:Key="BlackLight">#64666C</Color>
    <SolidColorBrush x:Key="BlackLightBrush" Color="{StaticResource BlackLight}" />

    <Color x:Key="BlackDark">#34363C</Color>
    <SolidColorBrush x:Key="BlackDarkBrush" Color="{StaticResource BlackDark}" />

    <Color x:Key="BlackPure">#000000</Color>
    <SolidColorBrush x:Key="BlackPureBrush" Color="{StaticResource BlackPure}" />

</ResourceDictionary>